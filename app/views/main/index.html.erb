<h1>HOME PAGE</h1>
<% if @user  %>
    <p>Hello! Welcome Back <%= @user.username %></p>
<% end %>

<% if @user  %>
    <%= button_to "Log Out", logout_path, method: :delete %>
<% else %>
    <button><%= link_to "Sign Up", "/signup" %></button>
    <button><%= link_to "Sign In", "/signin" %></button>
<% end %>
